{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "CoOwner.Dev",
    "Key": "super-secret-change-this-very-long-256bit",
    "AudiencePublic": "public"
  },
  "ReverseProxy": {
    "Routes": {
      "auth": {
        "ClusterId": "auth",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      },
      "groups": {
        "ClusterId": "groups",
        "Match": {
          "Path": "/groups/{**catch-all}"
        }
      },
      "vehicles": {
        "ClusterId": "vehicles",
        "Match": {
          "Path": "/vehicles/{**catch-all}"
        }
      },
      "bookings": {
        "ClusterId": "bookings",
        "Match": {
          "Path": "/bookings/{**catch-all}"
        }
      },
      "finance": {
        "ClusterId": "finance",
        "Match": {
          "Path": "/finance/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth": {
        "Destinations": {
          "d1": {
            "Address": "https://localhost:7441"
          }
        }
      },
      "groups": {
        "Destinations": {
          "d1": {
            "Address": "https://localhost:7442"
          }
        }
      },
      "vehicles": {
        "Destinations": {
          "d1": {
            "Address": "https://localhost:7443"
          }
        }
      },
      "bookings": {
        "Destinations": {
